{
  "compilerOptions": {
    "target": "es6",
    "strict": true,
    "module": "commonjs",
    "sourceMap": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
  },
  "include": ["**/*.ts", "./**/*.json"]
}